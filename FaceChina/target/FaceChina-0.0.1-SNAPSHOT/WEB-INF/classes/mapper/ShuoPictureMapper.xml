<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fc.main.dao.ShuoPictureDao">
	<!-- 基于shuoId查找pictureid -->
  	<insert id="insertShuoPicture">
		insert into shuo_picture
		(shuo_picture_id,shuo_picture_shuoid,shuo_picture_pictureid) 
		values
		(null,#{shuo_picture_shuoid},#{shuo_picture_pictureid}) 
	</insert>
</mapper>